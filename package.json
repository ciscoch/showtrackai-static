{
  "name": "showtrackai-static",
  "version": "1.0.0",
  "description": "ShowTrackAI - Elite livestock management app for FFA students (Static Site)",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run build:static",
    "build:static": "npm run pre-build && next build && npm run post-build",
    "build:production": "npm run build:static",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "pre-build": "node scripts/pre-build.cjs",
    "post-build": "node scripts/post-build.cjs",
    "validate-build": "node scripts/validate-build.cjs",
    "generate-icons": "node scripts/generate-icons.cjs",
    "serve": "npx serve out -l 3001",
    "analyze": "npm run build && npx @next/bundle-analyzer out",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "deploy:netlify": "npm run build && netlify deploy --prod --dir=out",
    "deploy:vercel": "npm run build && vercel --prod",
    "deploy:github": "npm run build && gh-pages -d out",
    "deploy:surge": "npm run build && surge out",
    "deploy:aws": "npm run build && aws s3 sync out/ s3://your-bucket-name --delete"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.53.0",
    "@tanstack/react-query": "^5.59.20",
    "lucide-react": "^0.456.0",
    "next": "^15.4.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.2",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.4.5",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.4.5",
    "fs-extra": "^11.2.0",
    "gh-pages": "^6.2.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "sharp": "^0.33.5",
    "sitemap": "^8.0.0",
    "vercel": "^39.2.4"
  },
  "keywords": [
    "FFA",
    "agriculture",
    "livestock",
    "education",
    "PWA",
    "static-site",
    "Next.js",
    "React",
    "Supabase"
  ],
  "author": "ShowTrackAI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ciscoch/showtrackai-static.git"
  },
  "homepage": "https://ciscoch.github.io/showtrackai-static",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}